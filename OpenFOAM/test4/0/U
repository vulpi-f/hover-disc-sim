/*--------------------------------*- C++ -*----------------------------------*/

FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
addLayers
;
dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (0 0 0);
boundaryField
{
    inlet_cushion   { type fixedValue; value uniform (0 0 -5); }   // esempio: -z in ingresso
    inlet_annulus   { type fixedValue; value uniform (0 0 -50); }  // esempio: getto pi√π veloce
    cushionTopWall  { type fixedValue; value uniform (0 0 0); }    // top chiuso nel cuscino
    bottomWall      { type fixedValue; value uniform (0 0 0); }
    innerAxis       { type symmetryPlane; }
    wedgeLow        { type wedge; }
    wedgeHigh       { type wedge; }

    outerWall
    {
    type        inletOutlet;
    inletValue  uniform (0 0 0);
    value       uniform (0 0 0);
    }
}
