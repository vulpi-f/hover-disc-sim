/*--------------------------------*- C++ -*----------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (0 0 0);

boundaryField
{
    inlet_annulus   { type fixedValue; value uniform (0 0 -50); }
    inlet_cushion   { type fixedValue; value uniform (0 0 -5); }

    cushionTopWall  { type noSlip; }
    bottomWall      { type noSlip; }

    wedgeLow        { type wedge; }
    wedgeHigh       { type wedge; }

    innerAxis       { type symmetry; }         // << match con patch "symmetry"

    outerWall
    {
        type            zeroGradient;
    }
}
