FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
addLayers
;
dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (0 0 0);
boundaryField
{
    inlet_annulus   { type fixedValue; value uniform (0 0 -50); }
    inlet_cushion   { type fixedValue; value uniform (0 0 -5); }
    cushionTopWall  { type noSlip; }
    bottomWall      { type noSlip; }
    wedgeLow        { type wedge; }
    wedgeHigh       { type wedge; }

    innerAxis       { type symmetry; }          // <<-- COERENTE con patch "symmetry"
    outerWall       { type freestream; freestreamValue uniform (0 0 0); }
}
