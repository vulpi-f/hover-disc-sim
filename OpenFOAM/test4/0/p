FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 0;
boundaryField
{
    inlet_cushion     { type zeroGradient; }
    inlet_annulus     { type zeroGradient; }
    cushionTopWall    { type zeroGradient; }
    bottomWall        { type zeroGradient; }
    innerAxis         { type symmetryPlane; }
    wedgeLow          { type wedge; }
    wedgeHigh         { type wedge; }

    outerWall { type zeroGradient; } 
}
