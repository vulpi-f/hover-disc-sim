FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 0;
boundaryField
{
    inlet_annulus   { type zeroGradient; }
    inlet_cushion   { type zeroGradient; }
    cushionTopWall  { type zeroGradient; }
    bottomWall      { type zeroGradient; }
    wedgeLow        { type wedge; }
    wedgeHigh       { type wedge; }

    innerAxis       { type symmetry; }          // <<-- COERENTE con patch "symmetry"
    outerWall       { type freestream; freestreamValue uniform 0; }  // se usi far-field
}
